{
    "extends": "airbnb-base",
    "env": {
        "es6": true,
        "browser": true
    },
    "rules": {
        "indent": ["error", 4],
        "no-unused-expressions": 0,
        "max-len": ["error", 120],
        "arrow-parens": 0,
        "no-confusing-arrow": 0,
        "no-param-reassign": 0
    }
}